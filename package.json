{
  "name": "ngx-filesize-demo",
  "description": "A filesize.js pipe for Angular",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-filesize-demo",
    "build_lib": "ng build ngx-filesize && npm run readme-copy",
    "test": "ng test ngx-filesize-demo",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless ngx-filesize-demo",
    "test_lib": "ng test ngx-filesize",
    "test_lib-headless": "ng test --watch=false --browsers=ChromeHeadless ngx-filesize",
    "publish_lib": "npm run build_lib && cd \"dist/ngx-filesize\" && npm publish",
    "readme-copy": "cp \"./README.md\" \"./dist/ngx-filesize\""
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@angular/animations": "^14.2.3",
    "@angular/common": "^14.2.3",
    "@angular/compiler": "^14.2.3",
    "@angular/core": "^14.2.3",
    "@angular/platform-browser": "^14.2.3",
    "@angular/platform-browser-dynamic": "^14.2.3",
    "filesize": "^9.0.11",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.3",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.1",
    "typescript": "~4.8.4"
  }
}
